<head>
<title> Testing </title>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>

<body>
<div class="container">
<div class="table table-responsive">
				<table class="table table-striped table-bordered">
					<thead class="thead-inverse">
						<tr>
							<th>#</th>
							<th>Tag</th>
							<th>Name</th>
							<th>Week1 given</th>
							<th>Week1 received</th>
							<th>Week2 given</th>
							<th>Week2 received</th>
							<th>Week3 received</th>
							<th>Week4 received</th>
							<th>Week4 received</th>
							<th>Week5 received</th>
							<th>Week5 received</th>
						</tr>
					</thead>
					
					<tbody>
					<% rows.forEach((player) => {%>
					<tr>
						<td><%= player.sr %></td>
						<td><%= player.tag %></td>
						<td><%= player.tag %></td>
						<td><%= player.week1given %></td>
						<td><%= player.week1received %></td>
						<td><%= player.week2given %></td>
						<td><%= player.week2received %></td>
						<td><%= player.week3given %></td>
						<td><%= player.week3received %></td>
						<td><%= player.week4given %></td>
						<td><%= player.week4received %></td>
						<td><%= player.week5given %></td>
						<td><%= player.week5received %></td>
					</tr>
					<% }) %>
					</tbody>
				</table>
			</div>
</div>